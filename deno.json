{
  "imports": {
    "react": "https://esm.sh/react@19.0.0",
    "react-dom": "https://esm.sh/react-dom@19.0.0",
    "react-icons/fi": "https://esm.sh/react-icons@5.4.0/fi",
    "react-router-dom": "https://esm.sh/react-router-dom@7.1.5",
    "react/jsx-runtime": "https://esm.sh/react@19.0.0/jsx-runtime",
    "react-dom/client": "https://esm.sh/react-dom@19.0.0/client"
  },
  "tasks": {
    "build": "deno run --allow-all build.ts",
    "start": "deno run --allow-net --allow-read server.ts",
    "dev": "deno run --allow-net --allow-read --watch server.ts"
  },
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "lint": {
      "rules": {
        "exclude": ["no-explicit-any", "no-unused-vars"]
      }
    }
  },
  "unstable": [
    "sloppy-imports"
  ]
}
